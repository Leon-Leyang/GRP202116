<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.grp202116.mapper.ModelMapper">

    <resultMap id="interfaceMap" type="com.grp202116.backend.pojo.LabelInterfaceDO">
        <id column="code" property="code"/>
        <result column="project_id" property="projectId"/>
        <result column="create_time" property="createTime"/>
    </resultMap>

    <insert id="insert" parameterType="LabelInterfaceDO">
        insert into labelinterface (code, project_id, create_time)
        values (#{code}, #{projectId}, #{createTime})
    </insert>

    <select id="getByProjectId" resultMap="interfaceMap">
        select *
        from labelinterface
        where project_id = #{projectId}
    </select>

    <delete id="deleteByProjectId">
        delete
        from labelinterface
        where project_id = #{projectId}
    </delete>

</mapper>